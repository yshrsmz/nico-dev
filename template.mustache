{{#legacy}}
%{{&spIdentifier}} {
   background-image: url("{{&imagePath}}");
   background-repeat: no-repeat;
   background-size: {{imageWidth}}px {{imageHeight}}px;
}

{{#styles}}
@mixin {{&name}} {
    @extend %{{&spIdentifier}};
    width: {{width}}px;
    height: {{height}}px;
    background-position: {{x}}px {{y}}px;
}
{{/styles}}
{{/legacy}}
